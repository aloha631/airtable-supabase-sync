{
  "name": "airtable-syc-to-supabase",
  "version": "1.0.0",
  "description": "Airtable to Supabase sync service with AI integration",
  "type": "module",
  "main": "dist/sync-service.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/sync-service.ts",
    "start": "npm run build && node dist/scheduler.js",
    "airtable-export": "ts-node --esm src/airtable-export-v2.ts",
    "csv-import": "ts-node --esm src/csv-import-v2.ts",
    "sync": "npm run build && node dist/sync-service.js",
    "incremental-sync": "npm run build && node dist/incremental-sync.js",
    "check-sync-history": "npm run build && node dist/check-sync-history.js",
    "test-email": "npm run build && node dist/test-email.js",
    "test-query-performance": "npm run build && node dist/test-query-performance.js",
    "test-jsonb-links": "npm run build && node dist/test-jsonb-links.js",
    "test-airtable": "npm run build && node dist/test-airtable-connection.js",
    "start-sync-service": "npm run build && node dist/scheduler.js"
  },
  "keywords": [
    "airtable",
    "supabase",
    "sync",
    "ai"
  ],
  "author": "Liaosanyi",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "airtable": "^0.12.2",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.3",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
